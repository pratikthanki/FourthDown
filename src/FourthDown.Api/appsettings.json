{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "Kestrel": {
        "EndPoints": {
            "Https": {
                "Url": "http://*:5000"
            }
        }
    },
    "Telemetry": {
        "UseApplicationInsights": false,
        "UseOpenTelemetry": true,
        "ApplicationInsightsInstrumentationKey": "",
        "ApplicationInsightsForOpenTelemetryInstrumentationKey": ""
    },
    "AllowedHosts": "*",
    "UseSampleAuth": true
}